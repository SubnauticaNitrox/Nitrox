<ResourceDictionary
    xmlns="https://github.com/avaloniaui"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <ResourceDictionary.ThemeDictionaries>
        <!-- Default/Light theme -->
        <ResourceDictionary x:Key="Default">
            <!-- Main colors -->
            <SolidColorBrush Color="#007BFF" x:Key="BrandPrimaryBrush" />
            <SolidColorBrush Color="#FFFFFF" x:Key="BrandWhiteBrush" />
            <SolidColorBrush Color="#000000" x:Key="BrandBlackBrush" />
            <SolidColorBrush Color="#D1D1D1" x:Key="BrandBorderBrush" />
            <SolidColorBrush Color="#ECECEC" x:Key="BrandControlBackgroundBrush" />
            <SolidColorBrush Color="#D9D9D9" x:Key="BrandCheckboxBackgroundBrush" />
            <SolidColorBrush Color="#151516" x:Key="BrandPanelBackgroundBrush" />
            <SolidColorBrush Color="#1EFFFFFF" x:Key="BrandBorderBackgroundBrush" />
            <SolidColorBrush Color="#000000" x:Key="BrandCaretBrush"/>
            <SolidColorBrush Color="#40000000" x:Key="BrandFocusBorderBrush"/>
            <SolidColorBrush Color="#7F000000" x:Key="BrandSubTextBrush"/>
            <SolidColorBrush Color="#CCCCCC" x:Key="BrandScrollThumbBrush"/>
            <SolidColorBrush Color="#8F8F8F" x:Key="BrandScrollThumbPointerOverBrush"/>
            <SolidColorBrush Color="#3F3F3F" x:Key="BrandScrollThumbPressedBrush"/>
            <SolidColorBrush Color="#38C149" x:Key="BrandOnColor"/>
            <SolidColorBrush Color="#FF5E57" x:Key="BrandOffColor"/>

            <!-- Notification colors -->
            <SolidColorBrush Color="#FF005C" x:Key="BrandAbortBrush" />
            <SolidColorBrush Color="#FFB3B3" x:Key="BrandAbortBackgroundBrush" />
            <SolidColorBrush Color="#E60914" x:Key="BrandErrorBrush" />
            <SolidColorBrush Color="#11AD45" x:Key="BrandSuccessBrush" />
            <SolidColorBrush Color="#007BFF" x:Key="BrandInformationBrush" />
            <SolidColorBrush Color="#F5A300" x:Key="BrandWarningBrush" />
        </ResourceDictionary>
        <ResourceDictionary x:Key="Dark">
            <SolidColorBrush Color="#000000" x:Key="BrandWhiteBrush" />
            <SolidColorBrush Color="#FFFFFF" x:Key="BrandBlackBrush" />
            <SolidColorBrush Color="#323232" x:Key="BrandBorderBrush" />
            <SolidColorBrush Color="#1D1D1D" x:Key="BrandControlBackgroundBrush" />
            <SolidColorBrush Color="#333333" x:Key="BrandCheckboxBackgroundBrush" />
            <SolidColorBrush Color="#FFFFFF" x:Key="BrandCaretBrush"/>
            <SolidColorBrush Color="#40FFFFFF" x:Key="BrandFocusBorderBrush"/>
            <SolidColorBrush Color="#7FFFFFFF" x:Key="BrandSubTextBrush"/>
            <SolidColorBrush Color="#262626" x:Key="BrandScrollThumbBrush"/>
            <SolidColorBrush Color="#3F3F3F" x:Key="BrandScrollThumbPointerOverBrush"/>
            <SolidColorBrush Color="#8F8F8F" x:Key="BrandScrollThumbPressedBrush"/>
        </ResourceDictionary>
    </ResourceDictionary.ThemeDictionaries>
</ResourceDictionary>
